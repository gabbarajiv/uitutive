{
    "name": "uitutive-backend",
    "version": "1.0.0",
    "description": "Backend API for Uitutive Form Generator - Node.js + Express",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node-esm src/server.ts",
        "dev:watch": "nodemon --exec ts-node-esm src/server.ts",
        "test": "jest",
        "lint": "eslint src --ext .ts",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "form-generator",
        "express",
        "typescript",
        "ollama",
        "ai"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@types/pg": "^8.15.6",
        "axios": "^1.6.0",
        "better-sqlite3": "^12.5.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "sqlite3": "^5.1.7",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.6",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
