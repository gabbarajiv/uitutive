# Docker Environment Configuration
NODE_ENV=production

# Database Configuration
DB_TYPE=sqlite
# DB_TYPE=postgres  # Uncomment to use PostgreSQL instead

# PostgreSQL (only if DB_TYPE=postgres)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=uitutive

# Ollama Configuration
OLLAMA_MODEL=llama2
# Other models: mistral, neural-chat, llama2-uncensored, orca-mini, etc.

# CORS Configuration
CORS_ORIGIN=http://localhost:4200
