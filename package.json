{
  "name": "uitutive",
  "version": "1.0.0",
  "description": "Monorepo: Form Generator with AI assistance - Angular Frontend + Node.js Backend",
  "scripts": {
    "frontend:start": "ng serve",
    "frontend:build": "ng build",
    "frontend:watch": "ng build --watch --configuration development",
    "frontend:test": "ng test",
    "backend:start": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "backend:dev:watch": "cd backend && npm run dev:watch",
    "backend:build": "cd backend && npm run build",
    "backend:test": "cd backend && npm run test",
    "backend:lint": "cd backend && npm run lint",
    "install:all": "npm install && cd backend && npm install",
    "build:all": "npm run frontend:build && npm run backend:build",
    "start:frontend": "npm run frontend:start",
    "start:backend": "npm run backend:dev:watch",
    "dev": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "mobile:build:web": "ng build --configuration mobile",
    "mobile:build:android": "npm run mobile:build:web && powershell -Command \"if (Test-Path 'android') { Remove-Item -Recurse -Force 'android' }\" && npx cap add android && npx cap sync android && npx cap open android",
    "mobile:build:ios": "npm run mobile:build:web && npx cap add ios && npx cap build ios && npx cap open ios",
    "mobile:dev": "concurrently \"npm run frontend:start\" \"npx cap sync\"",
    "mobile:sync:android": "npx cap sync android && npx cap open android",
    "mobile:sync:ios": "npx cap sync ios && npx cap open ios",
    "mobile:open:android": "npx cap open android",
    "mobile:open:ios": "npx cap open ios"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.0",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.3",
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "axios": "^1.13.2",
    "express": "^5.1.0",
    "openai": "^6.9.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.0",
    "@capacitor/cli": "^6.0.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@types/uuid": "^11.0.0",
    "concurrently": "^8.2.2",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}